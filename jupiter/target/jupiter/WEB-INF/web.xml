<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>SearchItem</servlet-name>
  	<display-name>SearchItem</display-name>
  	<description></description>
  	<servlet-class>rpc.SearchItem</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RecommendItem</servlet-name>
  	<display-name>RecommendItem</display-name>
  	<description></description>
  	<servlet-class>rpc.RecommendItem</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>SearchItem</servlet-name>
  	<url-pattern>/search</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RecommendItem</servlet-name>
  	<url-pattern>/recommendation</url-pattern>
  </servlet-mapping>
</web-app>
